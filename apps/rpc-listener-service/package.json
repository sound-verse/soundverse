{
  "name": "@soundverse/app-rpc-listener-service",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "MIT",
  "scripts": {
    "build": "rimraf dist && tsc -b",
    "dev": "tsc-watch -b --noClear --onSuccess \"node ./dist/main.js\"",
    "start:debug": "tsc-watch -b --onSuccess \"node --inspect-brk dist/main.js\"",
    "start": "node dist/main.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "lint": "eslint --ext js,jsx,ts,tsx ./src",
    "prettier": "prettier --check 'src/**/*.ts' --write"
  },
  "dependencies": {
    "@nestjs/common": "^8.0.0",
    "@nestjs/config": "^1.0.3",
    "@nestjs/core": "^8.0.0",
    "@nestjs/mongoose": "^7.2.4",
    "@nestjs/platform-express": "^8.0.0",
    "aws-sdk": "^2.1030.0",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.13.1",
    "config": "^3.3.7",
    "eslint-plugin-import": "^2.25.2",
    "ethers": "^5.5.1",
    "mongoose": "^5.10.15",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "web3": "^1.6.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.0.0",
    "@nestjs/schematics": "^8.0.0",
    "@nestjs/testing": "^8.0.0",
    "@soundverse/eslint-config": "1.0.0",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.0.1",
    "@types/node": "^16.0.0",
    "@types/supertest": "^2.0.11",
    "eslint": "^7.14.0",
    "jest": "^27.2.5",
    "prettier": "^2.3.2",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "^27.0.3",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsc-watch": "^4.4.0",
    "tsconfig-paths": "^3.10.1",
    "typescript": "^4.3.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
