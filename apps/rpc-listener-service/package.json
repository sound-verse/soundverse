{
  "name": "@soundverse/app-rpc-listener-service",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "MIT",
  "scripts": {
    "build": "rimraf dist && tsc -b",
    "dev": "tsc-watch -b --noClear --onSuccess \"node ./dist/main.js\"",
    "start:debug": "tsc-watch -b --onSuccess \"node --inspect-brk dist/main.js\"",
    "start": "node dist/main.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "lint": "eslint --ext js,jsx,ts,tsx ./src",
    "prettier": "prettier --check 'src/**/*.ts' --write"
  },
  "dependencies": {
    "@nestjs/common": "^8.4.1",
    "@nestjs/config": "^1.2.0",
    "@nestjs/core": "^8.4.1",
    "@nestjs/microservices": "^8.4.1",
    "@nestjs/mongoose": "^9.0.3",
    "@nestjs/platform-express": "^8.4.1",
    "amqp-connection-manager": "^4.1.1",
    "amqplib": "^0.8.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "crypto": "^1.0.1",
    "eslint-plugin-import": "^2.25.2",
    "ethers": "^5.6.2",
    "mongoose": "^6.2.6",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.5.5",
    "web3": "^1.7.1",
    "web3-eth-contract": "^1.7.1",
    "web3-utils": "^1.7.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.2.3",
    "@nestjs/schematics": "^8.0.8",
    "@nestjs/testing": "^8.4.1",
    "@soundverse/eslint-config": "1.0.0",
    "@soundverse/shared-rpc-listener-service": "0.0.1",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.21",
    "@types/supertest": "^2.0.11",
    "eslint": "^8.12.0",
    "jest": "^27.2.5",
    "prettier": "^2.5.1",
    "source-map-support": "^0.5.21",
    "supertest": "^6.2.2",
    "ts-jest": "^27.1.3",
    "ts-loader": "^9.2.8",
    "ts-node": "^10.7.0",
    "tsc-watch": "^4.6.2",
    "tsconfig-paths": "^3.14.0",
    "typescript": "^4.6.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
