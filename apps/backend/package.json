{
  "name": "@soundverse/app-api",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "MIT",
  "scripts": {
    "build": "rimraf dist && tsc -b",
    "dev": "tsc-watch -b --noClear --onSuccess \"node ./dist/main.js\"",
    "start:debug": "tsc-watch -b --onSuccess \"node --inspect-brk dist/main.js\"",
    "start": "node dist/main.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "lint": "eslint --ext js,jsx,ts,tsx ./src",
    "prettier": "prettier --check 'src/**/*.ts' --write"
  },
  "dependencies": {
    "@nestjs/axios": "^0.0.3",
    "@nestjs/common": "^8.2.6",
    "@nestjs/config": "^1.1.6",
    "@nestjs/core": "^8.2.6",
    "@nestjs/graphql": "^9.1.1",
    "@nestjs/jwt": "^8.0.0",
    "@nestjs/microservices": "^8.2.6",
    "@nestjs/mongoose": "^7.2.4",
    "@nestjs/passport": "^8.1.0",
    "@nestjs/platform-express": "^8.2.6",
    "@pinata/sdk": "^1.1.23",
    "amqp-connection-manager": "^4.0.1",
    "amqplib": "^0.8.0",
    "apollo-server-express": "^3.4.0",
    "aws-sdk": "^2.1030.0",
    "axios": "^0.24.0",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.13.1",
    "crypto": "^1.0.1",
    "eslint-plugin-import": "^2.25.2",
    "ethers": "^5.5.1",
    "file-type": "^16.5.3",
    "form-data": "^4.0.0",
    "graphql": "^15.5.0",
    "graphql-relay": "^0.9.0",
    "graphql-type-json": "^0.3.2",
    "graphql-upload": "^12.0.0",
    "ipfs-only-hash": "^4.0.0",
    "mongoose": "^5.10.15",
    "passport": "^0.5.0",
    "passport-jwt": "^4.0.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.5.2",
    "web3": "^1.6.1",
    "xss": "^1.0.10"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.2.0",
    "@nestjs/schematics": "^8.0.5",
    "@nestjs/testing": "^8.2.6",
    "@soundverse/eslint-config": "1.0.0",
    "@soundverse/shared-rpc-listener-service": "0.0.1",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.0.1",
    "@types/node": "^16.0.0",
    "@types/supertest": "^2.0.11",
    "eslint": "^7.14.0",
    "jest": "^27.2.5",
    "prettier": "^2.3.2",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "^27.0.3",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsc-watch": "^4.4.0",
    "tsconfig-paths": "^3.10.1",
    "typescript": "^4.3.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
